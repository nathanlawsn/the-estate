---
const { items } = Astro.props;
---

<ul class="hidden lg:flex items-center gap-8 font-medium text-lg">
    {
        items.map((item) => (
            <li>
                <a class="hover:underline" href={item.url}>
                    {item.title}
                </a>
            </li>
        ))
    }
</ul>
